<?xml version="1.0" encoding="UTF-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Simulator\Domain\Report\Model\Report" table="report" repository-class="App\Simulator\Domain\Report\Repository\ReportRepositoryInterface">
        <id name="id" type="string" column="id"/>
        <field name="date" column="date" type="datetime" />
        <field name="numTotalTraveledFloors" column="num_total_traveled_floors" type="integer" />
        <field name="numTraveledFloors" column="num_traveled_floors" type="integer" />
        <field name="currentFloor" column="current_floor" type="integer" />
        <field name="creationDate" column="creation_date" type="datetime" />
        <field name="elevatorId" column="elevator_id" type="string" />
        <field name="buildingId" column="building_id" type="string" />
    </entity>

</doctrine-mapping>